 generator client {
   provider        = "prisma-client-js"
   previewFeatures = ["postgresqlExtensions"]
   output          = "./prisma/generated/client"
 }

 datasource db {
   provider = "postgresql"
   url      = env("DATABASE_URL")
  extensions = [vector]
 }

 model Document {
   id      Int     @id @default(autoincrement())
   content String
  embedding Unsupported("vector")
 }